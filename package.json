{
  "name": "@jericirenej/object-filter",
  "description": "Return an object with filtered properties based on a supplied list of filter keys. Exclude/include filtering options and recursive operation available.",
  "author": "Irenej Jeric",
  "version": "1.2.0",
  "license": "ISC",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist/index.js",
    "dist/utils.js",
    "dist/*.d.ts",
    "README.md"
  ],
  "scripts": {
    "test": "jest",
    "dev": "tsc-watch",
    "prepublishOnly": "npm run compile",
    "compile": "npm run clean && tsc",
    "build": "tsc",
    "clean": "rm -rf dist/*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jericirenej/object-filter.git"
  },
  "bugs": {
    "url": "https://github.com/jericirenej/object-filter/issues"
  },
  "keywords": [
    "no-dependencies",
    "nodependencies",
    "zero-dependencies",
    "zerodependencies",
    "object",
    "object-filter",
    "javascript",
    "typescript",
    "filter",
    "object-trim",
    "trim"
  ],
  "prettier": {
    "arrowParens": "avoid"
  },
  "devDependencies": {
    "@types/jest": "^27.5.0",
    "@types/node": "^17.0.31",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "eslint": "^8.15.0",
    "jest": "^28.1.0",
    "ts-jest": "^28.0.1",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": ">=14.17.0"
  }
}
